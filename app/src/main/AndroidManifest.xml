<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mcqquiz.quizcode">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizCOde">
        <activity android:name=".QuestionsActivity"></activity>
        <activity android:name=".ActivityFirebase" />
        <activity android:name=".ActivityMysql" />
        <activity android:name=".SelectQuiz" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".ActivityPython" />
        <activity android:name=".Level" />
        <activity android:name=".ActivityKotlin" />
        <activity android:name=".ActivityJava" />
        <activity android:name=".ActivityJavascript" />
        <activity android:name=".ActivityHtml" />
        <activity android:name=".ActivityCsharp" />
        <activity android:name=".ActivityCpp" />
        <activity android:name=".ActivityC" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>